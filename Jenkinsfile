pipeline {
    agent any

    stages {
        stage ('dry stuff') {
            steps {
                echo "add floor"
                echo "add choco powder"
                echo "powdered sugar"
            }
        }
        stage ('wet stuff') {
            steps {
                echo "add milk"
                echo "add melted butter"
                echo "add yogurt"
            }
        }
        stage ('mix stuff') {
            steps {
                echo "mix wet and dry stuff without lumps"
            }
        }
        stage ('bake') {
            steps {
                echo "bake for 15 mins"
            }
        }
    }
}